{
  "name": "mycleverfriend",
  "version": "0.0.1",
  "description": "The open-source RPA Engine.",
  "main": "start.js",
  "scripts": {
    "start": "nodemon ./dist/mycleverfriend.js | bunyan",
    "eslint": "eslint --fix --ext .js src/",
    "babel": "babel src --out-file dist/mycleverfriend.js --copy-files",
    "build": "npm run eslint && npm run babel",
    "dev": "watch 'npm run build' src --wait 3 --ignoreDotFiles"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daudiihhdau/mycleverfriend.git"
  },
  "keywords": [
    "RPA",
    "Automation"
  ],
  "author": "Mathias Mielitz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/daudiihhdau/mycleverfriend/issues"
  },
  "homepage": "https://github.com/daudiihhdau/mycleverfriend#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "eslint": "^5.3.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "watch": "^1.0.2"
  },
  "dependencies": {
    "app-root-path": "^2.1.0",
    "bunyan": "^1.8.12",
    "chalk": "^2.4.1",
    "knex": "^0.15.2",
    "lokijs": "^1.5.5",
    "ora": "^3.0.0",
    "tree-model": "^1.0.7"
  }
}
