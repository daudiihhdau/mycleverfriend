{
  "version": 1,
  "info": {
    "name": "My Favorites On Amazon",
    "version": "1.0.0.1",
    "author": "Mathias Mielitz",
    "description": "Copy a directory or file.",
    "tags": ["directory", "copy"]
  },
  "collections": { 
    "csvfiles": [
      {
        "filename": "/home/daudiihhdau/Downloads/test1.csv",
        "enabled": true
      },
      {
        "filename": "/home/daudiihhdau/Downloads/test99.csv",
        "enabled": false
      }
    ]
  },
  "plugins": [
    { "name": "@mycleverfriend/readcsvplugin", "version": "1.0.0" }
  ],
  "actions": [
    {
      "plugin_id": 0,
      "packages": {
        "files": [
          {
            "select": ["filename", "enabled AS   is_enabled"],
            "from": "csvfiles",
            "where": { "enabled": true }
          }
        ]
      }
    }
  ]
}